
class Efecto{
    + sumarPuntaje(Puntaje puntaje): Puntaje
    + agregarEfecto(Efecto otroEfecto)
}

class Puntaje{
  - puntos : Int
  - multiplicador: int 
  +Puntaje()
  + Puntaje(Int puntos,Int multiplicador, Int fichas)
  + agregarPuntos(Int puntos)
  + agregarMultiplicador(Int multiplicador)
  + agregarFichas(Int fichas)
